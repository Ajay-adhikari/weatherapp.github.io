<body>
    <div class="container" >
        
        
        <div class="wcloud">
            <div class="cloud1" style="background-color: white; box-shadow: none"></div>
            <div class="cloud2" style="background-color: white; box-shadow: none"></div>
            <div class="cloud3" style="background-color: white; box-shadow: none"></div>
          </div>
          <div class="wcloud2">
            <div class="cloud1" style="background-color: white; box-shadow: none"></div>
             <div class="cloud2" style="background-color: white; box-shadow: none"></div>
            <div class="cloud3" style="background-color: white; box-shadow: none"></div>
          </div>
        <div class="wave-one"></div>
        <div class="wave-two"></div>
        <div class="wave-three"></div>

        <p id="weathercon"><i class="fa-solid fa-sun" style="color
        :#eccc68  " aria-hidden="true" ></i>
        </p>

        <div class="info" >

            <h2 id="location"><i class="fas fa-street-view"></i><SPAn id="loca">enter again</SPAn> </h2>
            
            <p id="date"> |month|date |00:00</p>
            <h1 id="temp">0&#xb0</h1>
            
            <p id="tempminmax" >Min 0&#xb0 | Max 0&#xb0</p>
        </div>
        


    </div>

